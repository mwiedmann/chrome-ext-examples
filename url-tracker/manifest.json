{
  "short_name": "URL Tracker",
  "name": "URL Tracker",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "URL Tracker"
  },
  "permissions": ["tabs", "storage"],
  "version": "0.1"
}
